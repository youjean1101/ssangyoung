<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd" >	
<mapper namespace="boardexp.A03_Dao">
	<select id="boardexpList" resultType="boardexp" parameterType="boardexp_seq">
		select *
		from boardexp
		WHERE 1=1
		AND bno LIKE '%'||''||'%'
		order by credte desc
	</select>
</mapper>		
		